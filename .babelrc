{
  "env": {
    "development": {
      "presets": [
        "@babel/preset-env"
      ],
      "plugins": [
        [
          "component",
          {
            "libraryName": "element-ui",
            "styleLibraryName": "theme-chalk"
          }
        ],
        ["@babel/plugin-transform-runtime", {
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": true
        }],
        ["@babel/plugin-proposal-decorators", {
          "legacy": true
        }],
        ["@babel/plugin-proposal-class-properties", {
          "loose": true
        }]
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-env"
      ],
      "plugins": [
        [
          "component",
          {
            "libraryName": "element-ui",
            "styleLibraryName": "theme-chalk"
          }
        ],
        ["@babel/plugin-transform-runtime", {
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": true
        }],
        ["@babel/plugin-proposal-decorators", {
          "legacy": true
        }],
        ["@babel/plugin-proposal-class-properties", {
          "loose": true
        }]
      ]
    },
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "current"
          }
        }]
      ]
    }
  }
}